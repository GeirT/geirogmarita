ruby '2.4.2'

# Where to look for gems
source 'https://rubygems.org' do
  # Version specifiers:
  #   ~> updates y in 1.0.y (recommended)
  #   ~> updates x in 1.x
  #   => updates x and y in 1.x.y
  #   <, >, <=, >=
  #   =  doesn't update
  #   No version, updates to latest (can be risky)
  gem 'github-pages', '~> 172.0.0'
end
